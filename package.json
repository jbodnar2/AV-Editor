{
  "name": "av-editor",
  "version": "0.0.0",
  "private": "true",
  "type": "module",
  "scripts": {
    "start": "npx live-server ./docs --port=3000 --host=localhost",
    "json-server": "npx json-server --port 3001 ./db.json",
    "start-php": "php -S localhost:3002 -t ./docs",
    "prettier": "npx prettier --write ."
  },
  "prettier": {
    "singleQuote": false,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "printWidth": 120,
    "tabWidth": 2,
    "endOfLine": "auto",
    "useTabs": false,
    "semi": true
  }
}
